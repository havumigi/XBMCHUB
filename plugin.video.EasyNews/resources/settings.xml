    <setting type="sep" />
	<setting id="mresults" type="number" label="Maximum Results:" default="1000" enable="!eq(-1,false)"/>
	<setting id="mlangex" type="text" label="Language Exclusions:" default="" enable="!eq(-3,false)"/>
	<setting id="mreso" type="enum" values="Any|720p|1080p|" label="Choose Resolution:" default="0" />
    <setting type="sep" />
	<setting id="mspam" type="bool" label="Enable Spam Filter:" default="true"/>
	<setting id="mrem" type="bool" label="Remove Duplicates:" default="true"/>
	<setting id="mgrex" type="bool" label="Enable Group Exclusion:" default="false"/>
	<setting id="msubject" type="text" label="Subject:" default="" enable="!eq(-2,false)"/>
	<setting id="mposter" type="text" label="Poster:" default="" enable="!eq(-3,false)"/>
	<setting id="mnewsgroup" type="text" label="Newsgroup:" default="" enable="!eq(-4,false)"/>
	<setting id="mfilename" type="text" label="Filename:" default="" enable="!eq(-5,false)"/>
	<setting id="mvcodec" type="text" label="Video Codec:" default="" enable="!eq(-6,false)"/>
	<setting id="macodec" type="text" label="Audio Codec:" default="" enable="!eq(-7,false)"/>
</category>
