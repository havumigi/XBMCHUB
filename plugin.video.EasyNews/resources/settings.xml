<settings>
<category label="Xbmc Account">
	<setting id="xbmchub" type="bool" label="XBMCHUB ACCOUNT" default="false"/>
	<setting id="xbmcuser" type="text" label="Username" default="" enable="!eq(-1,false)"/>
	<setting id="xbmcpass" type="text" option="hidden" label="Password" default="" enable="!eq(-2,false)"/>
</category> 	
<category label="Easy News">
	<setting id="fanart" type="bool" label="Enable Movie & TV Fanart" default="false"/>
	<setting id="mufanart" type="bool" label="Enable Music Fanart" default="false"/>
	<setting id="easy_account" type="bool" label="30300" default="false"/>
	<setting id="easy_user" type="text" label="Username" default="" enable="!eq(-1,false)"/>
	<setting id="easy_pass" type="text" option="hidden" label="Password" default="" enable="!eq(-2,false)"/>
    <setting id="boost" type="enum" values="Easyboost4|Easyboost1|Easyboost5|Non" label="Easyboost" default="0" />
    <setting type="sep" />
	<setting id="imdb_user" type="text" label="IMDB User Number" default="ur********" enable="!eq(-1,false)"/>
</category> 	
<category label="Movies">
	<setting id="mfileext" type="enum" values="Any|AVI|MKV|MP4|ISO|DIVX|MPG|FLV|WMV|MOV|ASF|RM" label="File Extension:" default="2" />
	<setting id="mfilesize" type="enum" values="Any|1MB|5Mb|15MB|25Mb|30Mb|40Mb|50Mb|75Mb|100Mb|200Mb|300Mb|400Mb|500Mb|750M|1Gb|1.5Gb|2Gb|2.5Gb|3Gb|3.5Gb|4Gb|5GB|6Gb|7GB|8Gb" label="Minimum File Size" default="0" />
